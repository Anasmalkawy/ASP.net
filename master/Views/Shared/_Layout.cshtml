					<ul>
						<li><a href="">طلباتى</a></li>
						<li><a href="">متابعه الطلبات</a></li>
						<li><a href="">الإرجاع والاستبدال</a></li>
						<li><a href="">حسابى</a></li>
						<li><a href="">معلوماتى</a></li>
					</ul>
				</div>

				<!-- col-footer 1 -->
				<div class="col-footer">
					<h4>ابقى على تواصل</h4>
					<div class="contact-details">
						<div class="contact-detail">
							<i class='bx bxs-map'></i>
							<span>فلسطين, غزة </span>
						</div>
						<div class="contact-detail">
							<i class='bx bxs-envelope'></i>
							<span>mhmod@gmail.com </span>
						</div>
						<div class="contact-detail">
							<i class='bx bxs-phone-call'></i>
							<span>+ 970 546 6345 </span>
						</div>

					</div>
				</div>
			</div>
		</div>
		<div class="copy">
			<p> &copy;   جميع الحقوق محفوظة. صنع بواسطة محمود الشاعر ❤️</p>
		</div>
	</footer>
	<script src="~/js/main.js"></script>
	<script src="~/lib/jquery/dist/jquery.min.js"></script>
	<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
	<script src="~/js/site.js" asp-append-version="true"></script>
	@await RenderSectionAsync("Scripts", required: false)
</body>
</html>
